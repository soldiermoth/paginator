{
  "manifest_version": 2,
  "name":             "Paginator Up",
  "description":      "Navigates to next page by incrementing a number in the URL",
  "version":          "1.0",
  "browser_action":   { "default_icon": "up.png" },
  "permissions":      [ "activeTab", "tabs" ],
  "background":       { "scripts": ["up.js"] },
  "icons":            { "128": "logo-128.png" }
}
